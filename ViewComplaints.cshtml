@using Restaurant.Models
@{ 
    var first_20 = (List<ComplaintModel>)ViewBag.first_20;
}
<table style="margin-top:2em;" class="table table-bordered" style="width:100%;">
    <thead>
        <tr>
            <th>Date</th>   
            <th>Name</th>
            <th>Item</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody id="table_body">
        @foreach (var item in first_20)
        {
            <text>
                <tr>
                    <td>@item.Date.ToShortDateString()</td>
                    <td>@item.Name</td>
                    <td>@item.Comp</td>
                    <td>@item.Details</td>
                </tr>
            </text>
        }
    </tbody>
</table>
